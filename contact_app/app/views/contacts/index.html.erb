
<%= link_to "Creat a New Contact", "/contacts/new" %></button>

<h1>Your Contacts</h1>
<% @contacts.each do |contact| %>
  <div>
    <h2><%= link_to "#{contact.first_name} #{contact.middle_name} #{contact.last_name}", "/contacts/#{contact.id}" %></h2>
    <h3>Email: <%= contact.email %></h3>
    <h3>Bio: <%= contact.bio %></h3>
    <h3>latitude, longitude: <%= contact.latitude %>, <%= contact.longitude %></h3>
<%= link_to "Edit", "/contacts/#{contact.id}/edit" %>



<% end %>
